<Window x:Class="AutomateClickerBrielina.Controls.CliquesAdionador"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutomateClickerBrielina.Controls"
        mc:Ignorable="d"
        Title="CliquesAdionador" Height="300" Width="350">
    <Grid Background="LightGray">
        <StackPanel 
            x:Name="AdicionarCliquePanel" 
            Visibility="Visible"
            Margin="5"
            HorizontalAlignment="Center"
            VerticalAlignment="Center">
            <Frame x:Name="JanelaCliquesAdionador" />
        </StackPanel>
    </Grid>
</Window>