<Window x:Class="AutomateClickerBrielina.Controls.Transparente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutomateClickerBrielina.Controls"
        mc:Ignorable="d"
        Title="Transparente" Height="450" Width="800"
        WindowStyle="None" AllowsTransparency="True">
    <Grid>
        <StackPanel x:Name="backgroundPanel" Background="Green" >
            <Image x:Name="imagePanel" Opacity="0.8" Source="../Prints/lastPrint.png" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
